<%= form_for user do |f| %>
  <%= f.label :email %><br>
  <%= @user.email %><br><br>
  <%= f.label "First Name" %>
  <%= f.text_field :first_name %>
  <%= f.label "Last Name" %>
  <%= f.text_field :last_name %>
  <%= f.label "Monthly Income" %>
  <%= f.text_field :income %>
  <%= f.submit %>
<% end %>

